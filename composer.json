{
  "name": "ingenerator/k8s-composer-installer",
  "description": "Composer plugin to install k8s base manifests",
  "authors": [
    {
      "name": "Craig Gosman",
      "email": "craig@ingenerator.com"
    }
  ],
  "type": "composer-plugin",
  "require": {
    "composer-plugin-api": "^2.0"
  },
  "require-dev": {
    "composer/composer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Ingenerator\\K8s\\ComposerPlugin\\": "src/"
    }
  },
  "extra": {
    "class": "Ingenerator\\K8s\\ComposerPlugin\\K8sBasesInstallerPlugin"
  },
  "minimum-stability": "stable"
}
